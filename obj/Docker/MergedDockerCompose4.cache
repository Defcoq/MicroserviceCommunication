services:
  bookcart.api:
    build:
      context: C:\Worksapce\Dot.netCoreSample\Hands-On RESTful Web Services with ASP.NET
        Core 3\DemoLocal\BookShop.API
      dockerfile: src/BookCart.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: BookCart.API
      target: base
    container_name: BookCart.API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages;/root/.nuget/fallbackpackages2
    image: bookcartapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages --additionalProbingPath
        /root/.nuget/fallbackpackages2  "bin/Debug/netcoreapp3.1/BookCart.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Worksapce\Dot.netCoreSample\Hands-On RESTful Web Services with ASP.NET Core
      3\DemoLocal\BookShop.API\src\BookCart.API:/app:rw
    - C:\Users\jpierre.deffofotso\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\jpierre.deffofotso\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\jpierre.deffofotso\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Microsoft\Xamarin\NuGet:/root/.nuget/fallbackpackages:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages2:ro
    - C:\Users\jpierre.deffofotso\.nuget\packages:/root/.nuget/packages:ro
    - C:\Worksapce\Dot.netCoreSample\Hands-On RESTful Web Services with ASP.NET Core
      3\DemoLocal\BookShop.API:/src:rw
